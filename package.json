{
  "name": "mcd-unified-hrms",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "server": "tsx server/index.ts",
    "preml": "lsof -ti:7002 | xargs kill -9 2>/dev/null || true",
    "ml": "cd ml_service && ../.venv/bin/python main.py",
    "start": "concurrently -k \"npm:ml\" \"npm run server\" \"npm run dev\"",
    "start:web": "concurrently \"npm run server\" \"npm run dev\"",
    "lint:types": "tsc --noEmit",
    "audit:fix": "npm audit fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@openrouter/sdk": "^0.1.0",
    "@sentry/node": "^7.120.4",
    "@sentry/profiling-node": "^7.120.4",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.0",
    "helmet": "^8.0.0",
    "isomorphic-dompurify": "^2.35.0",
    "jsonwebtoken": "^9.0.3",
    "jspdf": "2.5.1",
    "lucide-react": "^0.562.0",
    "morgan": "^1.10.0",
    "pg": "^8.16.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^3.6.0",
    "tsx": "^4.19.2",
    "uuid": "^11.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.7",
    "@types/node": "^22.14.0",
    "@types/pg": "^8.16.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vercel/node": "^5.5.16",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^2.1.8",
    "concurrently": "^9.2.1",
    "jsdom": "^25.0.1",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^2.1.8"
